{
    "package_version": 2,
    "version": "1.0.0",
    "name": "script-help",
    "description": "i18n:script-help.description",
    "main": "./dist/main.js",
    "devDependencies": {
        "@types/node": "^16.0.1",
        "typescript": "^4.3.4"
    },
    "author": "zhh",
    "editor": ">=3.7.3",
    "scripts": {
        "build": "tsc -b",
        "watch": "tsc -w"
    },
    "contributions": {
        "menu": [
            {
                "path": "测试",
                "label": "i18n:script-help.menu.fullParent",
                "message": "log"
            }
        ],
        "messages": {
            "log": {
                "methods": [
                    "log"
                ]
            },
            "selection:select" : {
                "methods": [
                    "mainReady"
                ]
            },
            "assets:add" : {
                "methods": [
                    "assetsAdd1"
                ]
            }
        },
        "scene": {
            "script": "./dist/scene.js"
        },
        "assets": {
            "menu": {
                "methods": "./dist/assets-menu.js",
                "createMenu": "onCreateMenu",
                "assetMenu": "onAssetMenu",
                "dbMenu": "onDBMenu",
                "panelMenu": "onPanelMenu"
            }
        },
        "inspector": {
            "section": {
                "node": {
                    "CustomLabelComponent": "./dist/comp-label.js"
                }
            }
        },
        "hierarchy": {
            "menu": {
                "methods": "./dist/assets-hierarchy.js",
                "createMenu": "onCreateMenu",
                "rootMenu" : "onRootMenu",
                "nodeMenu": "onNodeMenu",
                "panelMenu": "onPanelMenu"
            }
        }
    }
}